#: import AKCircularProgress Modules.progresswidget.AKCircularProgress

<StretchImage@Image>
	canvas:
        Color:
            rgb: (1, 1, 1)
        Rectangle:
            texture: self.texture
            size: self.parent.size[0],1/self.image_ratio*self.parent.size[0]
            pos: self.x,(-1/self.image_ratio*self.parent.width+self.height)/2
<StartScreen>
    name:'Start'
    StretchImage:
        source: 'Data/PreSplash_BLUE.png'
        
    AKCircularProgress:
        id: progress_percent
        pos_hint: {"center_x": .5, "center_y": .1}
        size_hint: None, None
        size: dp(100), dp(100)
        start_deg: -120
        end_deg: 120
